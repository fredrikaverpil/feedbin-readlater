{
  "background": { "scripts": ["background.js"] },
  "browser_action": {
    "default_icon": "icons/icon-128.png",
    "default_title": "Feedbin: save for later reading"
  },
  "name": "Feedbin: save for leater reading",
  "description": "Send the current page to Feedbin for later reading.",
  "homepage_url": "https://github.com/fredrikaverpil/feedbin-readlater",
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "permissions": ["tabs", "http://*/*", "https://*/*"],
  "version": "0.1",
  "manifest_version": 2
}

